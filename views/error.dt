- void css(string file)
	link(rel= 'stylesheet', type='text/css', href='/css/#{file}.css')
doctype html
html
	head
		title Paste Code Service
		- css("site");
	body
		div.Header
			div.MyHeader
				a(href="/") #Paste-Code
		div#app
			div.MainContainer
				div.errorPageContainer
					div.errorPageText #{error_text}
					div.errorPageImg
						- if (error_code == "404")
							img(src='img/error_404.png')
						- else
							img(src='img/error_503.png')